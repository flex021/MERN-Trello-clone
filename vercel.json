{
  "redirects": [
    {
      "source": "/(.*)/",
      "has": [
        {
          "type": "host",
          "value": ".*"
        }
      ],
      "destination": "/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}